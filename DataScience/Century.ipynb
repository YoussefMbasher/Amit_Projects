    "def century(year):\n",
    "    if year % 100 == 0:\n",
    "        return year // 100\n",
    "    else:\n",
    "        return year // 100 + 1\n",
    "\n",
    "print(century(1705))  \n",
    "print(century(1900))  \n",
    "print(century(1601))  \n",
    "print(century(2000))  \n",
    "print(century(2742)) \n"
   ]
